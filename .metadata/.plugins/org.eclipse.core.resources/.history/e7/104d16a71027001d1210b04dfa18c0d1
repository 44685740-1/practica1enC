#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int numero;
	int i;
	int contadorPares;
	int contadorImpar;

	//inicializar
	contadorPares = 0;
	contadorImpar = 0;

	for(i=0; i<10;i++){
		printf(i);
		if(numero % 2 == 0){
			contadorPares++;
		} else{
			contadorImpar++;
		}
	}
	return 0;
}
