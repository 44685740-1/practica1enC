#include <stdio.h>
#include <stdlib.h>

int main(void) {
	setbuf(stdout, NULL);
	int parametro;
	printf("ingrese el parametro que quiere que se imprima");
	scanf("%d", parametro);

	return 0;
}

int MostrarParametro(parametro){
	int parametroParaImprimir;
	parametroParaImprimir = parametro;
	return parametroParaImprimir;
}
