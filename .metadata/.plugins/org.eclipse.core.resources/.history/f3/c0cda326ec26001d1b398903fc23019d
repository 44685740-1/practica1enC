#include <stdio.h>
#include <stdlib.h>

int main(void) {
	setbuf(stdout,NULL);
	int numero1;
	int numero2;
	int numero3;

	//%d es la d de digito ya que hablamos de una variable que es un numero entero
	printf("ingrese el primer numero");
	scanf("%d", &numero1);
	printf("ingrese le segundo numero");
	scanf("%d", &numero2);
	printf("ingrese el tercer numero");
	scanf("%d", &numero3);



	return 0;
}
