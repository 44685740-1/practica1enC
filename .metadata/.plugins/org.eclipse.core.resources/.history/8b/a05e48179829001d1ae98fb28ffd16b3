#include <stdio.h>
#include <stdlib.h>
int SumarDosEnteros(int numero1, int numero2){
	setbuf(stdout, NULL);
	int suma;
	suma = numero1 + numero2;
	return suma;
}
int main(void) {
	setbuf(stdout, NULL);
	int numero1;
	int numero2;
	int resultado;
	printf("ingrese el primer numero\n");
	scanf("%d", &numero1);
	printf("\ningrese el segundo numero");
	fflush(stdin);
	scanf("%d", &numero2);
	resultado = SumarDosEnteros(numero1,numero2);
	printf(resultado);
	return 0;
}




