#include <stdio.h>
#include <stdlib.h>

int main(void) {
	setbuf(stdout, NULL);
	int parametro;
	int retorno;
	printf("ingrese el parametro");
	scanf("%d", &parametro);
	retorno = MostrarParametro(parametro);
	return 0;
}

int MostrarParametro(int parametro){
	int retorno;
	retorno = parametro;
	return retorno;
}

