#include <stdio.h>
#include <stdlib.h>

int main(void) {
	setbuf(stdout, NULL);
	int importe;
	char paisDeOrigen;
	printf("ingrese el importe");
	scanf("%d", &importe);
	printf("ingres el pais de origen");
	scanf("%c", paisDeOrigen);
	return 0;
}
