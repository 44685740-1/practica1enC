#include <stdio.h>
#include <stdlib.h>

int main(void) {
	setbuf(stdout, NULL);
	int parametro;
	int imprimirEsto;
	printf("ingrese el parametro que quiere que se imprima\n");
	scanf("%d", &parametro);
	imprimirEsto = MostrarParametro(parametro);
	printf(imprimirEsto);
	return 0;
}

int MostrarParametro(int parametro){
	int parametroParaImprimir;
	parametroParaImprimir = parametro;
	return parametroParaImprimir;
}
